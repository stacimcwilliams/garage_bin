{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "consistent-return": [0],
    "arrow-body-style": [0],
    "camelcase": [0],
    "no-shadow": [0],
    "no-multi-assign": [0],
    "no-param-reassign": [0],
    "no-else-return": [0],
    "no-unused-expressions": [0],
    "no-unused-vars": [0],
    "import/no-unresolved": [0],
    "global-require": [0],
    "arrow-parens": [0],
    "func-names": [0],
    "prefer-arrow-callback": [0],
    "import/extensions": [0],
    "import/first": [0],
    "array-callback-return": [0],
    "no-undef": [0],
    "import/no-extraneous-dependencies": [0],
    "space-in-parens": [0],
    "no-unused-labels": [0],
    "no-plusplus": [0],
    "no-use-before-define": [0],
    "no-labels": [0],
    "no-restricted-syntax": [0],
    "no-label-var": [0],
    "no-path-concat": [0],
    "prefer-template": [0],
    "no-dupe-keys": [0],
  }
}
